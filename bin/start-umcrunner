#!/bin/bash
# umcrunnerd start up script 

# the script directory
CUR_DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"

# log directory
pwd=$(pwd)
cd $CUR_DIR/..
LOG_DIR=$(pwd)/logs/umcrunner
echo $LOG_DIR
mkdir -p $LOG_DIR

$CUR_DIR/umcrunnerd --runall metrics.conf >>$LOG_DIR/umcrunner-$HOSTNAME.log 2>&1 &

cd $pwd
